<div class="container">
	<!-- Static navbar -->
	<div class="navbar navbar-default" role="navigation">
		<div class="container-fluid">
			<div class="navbar-header {{{__ "logo"}}}">
				<a class="navbar-brand" href="../../"><img alt="إحصائيات التسجيل" src="../../img/hnec_logo.png"></a>
			</div>
			<div class="navbar-collapse" dir="rtl" >
				<ul class="nav navbar-nav {{{__ "navbar"}}}">
					<li class="active"><a href="../../">{{{__ "main"}}}</a></li>
				</ul>
			</div><!--/.nav-collapse -->
		</div><!--/.container-fluid -->
	</div>
	<div class="page-header">
  	<h1>{{{__ "pageheader1"}}}<br> <small>{{{__ "pageheader2"}}}</small></h1>
	</div>

	<!-- Main component for a primary marketing message or call to action -->
		
	<div class="panel panel-default">
		<div class="panel-heading">
	   	<ol class="breadcrumb {{{__ "textpos"}}}" dir="{{{__ "dir"}}}">
			  <li><a href="/">{{{__ "main"}}}</a></li>
			  <li><a href="/constituancy/{{constit_id}}">{{translate name_en name}}</a></li>
			</ol>

  	</div>
  	<div class="panel-body">
  		<div class="row-fluid">
  			<div dir = "{{{__ "dir"}}}">
		  		
		  		<table class="table table-bordered">
		  			<tbody>
		  				<tr>
		  					<td class="heads"> <b>{{{__ "ballot"}}}</b> : {{ballot.ballot_id}}-{{translate ballot.race_type ballot.race_type_ar}}</td>
		  					<td class="heads"><b>{{{__ "validv"}}}</b> : {{ballot.valid_votes}}</td>
		  					<td class="heads"><b>{{{__ "station"}}} : </b>{{ballot.stations}}</td>
						  	
						  	
		  				</tr>
		  				<tr>
		  					<td class="heads"><b>{{{__ "subconstit"}}} : </b>{{transs ballot.subconst_name}}</td>
						  	<td class="info"><b>{{{__ "seats"}}}</b> : {{ballot.seats}}</td>
						  	<td class="info"><b>{{{__ "completed"}}} : </b>{{ballot.completed}}</td>
		  				</tr>
		  				<tr>
		  					<td class="heads"><b>{{{__ "seconsubconstit"}}} : </b>{{transs ballot.subsubconst_name}}</td>
		  					<td class="info"><b>{{{__ "candidatecount"}}}</b> : {{ballot.candidate_count}}</td>
						  	<td class="info"><b>{{{__ "progress"}}} : </b> {{ballot.percentage}}%</td>

		  				</tr>
		  			</tbody>
		  		</table>

				</div>
			</div>
			<table  class="table table-striped table-condensed table-bordered " dir="{{{__ "dir"}}}" >
		    <thead>
		      <tr>
		      	<th>{{__ "number"}}</th>
		        <th>{{{__ "fullname"}}}</th>
		        <th>{{{__ "validv"}}}</th>
		        
		      </tr>
		    </thead>
		    <tbody>
		    {{#each candidates}}
		      <tr id="{{candidate_id}}" >
		      	<td>{{plusone @index}}</td>
		        <td>{{name}}</td>
		        <td>{{votes}}</td>
		        
		      </tr>
		    {{/each}}
		    </tbody>
		  </table>
		</div>
	</div>
</div> <!-- /container -->

<script src="{{{__ "js3"}}}"></script>
