<div class="container">

  <!-- Static navbar -->
  <div class="navbar navbar-default" role="navigation">
    <div class="container-fluid">
      <div class="navbar-header {{{__ "logo"}}}">
        <a class="navbar-brand" href="../../"><img alt="" src="./img/hnec_logo.png"></a>
      </div>
      <div class="navbar-collapse" dir="rtl" >
        <ul class="nav navbar-nav {{{__ "navbar"}}}">
          <!-- <li>
            <form class="navbar-form navbar-right" >
              
            </form>
          </li> -->
          <li> &nbsp&nbsp&nbsp</li>
          <li class="active"><a href="../../">{{{__ "cmain"}}}</a></li>
          <li><a href="http://candidates.hnec.ly/" target=_blank>{{{__ "candidates"}}}</a></li>
          <li><a href="http://hnec.ly" target=_blank>{{{__ "website"}}}</a></li>
        </ul>

      </div><!--/.nav-collapse -->
    </div><!--/.container-fluid -->
  </div>
  <div class="panel panel-default">
    <div class="panel-heading" dir = "{{{__ "dir"}}}">{{{__ "searchpanel"}}}</div>
    <div class="panel-body">
      <div class="row-fluid">
        <div dir = "{{{__ "dir"}}}">
          <div dir = "{{{__ "dir"}}}" class="row-fluid" >
            <div class="col-xs-2 {{{__ 'pullright'}}}">
              <form role="form">
                  <div class="form-group">
                    <label for="region">{{{__ "region"}}}</label>
                    <select class="form-control" id="region">
                      <option value="all">الكل</option>
                      {{#each centers}}
                      <option value="{{region}}">{{region}}</option>
                      {{/each}}
                    </select>
                  </div>
                </form>
            </div>
            <div class="col-xs-2 {{{__ 'pullright'}}}">
              <form role="form">
                <div class="form-group">
                  <label for="region">{{{__ "office"}}}</label>
                  <select class="form-control" id="offices">
                    <option value="all">الكل</option>
                  </select>
                </div>
              </form>
            </div>
            <div class="col-xs-4 {{{__ 'pullright'}}}">
              <form role="form">
                <div class="form-group">
                  <label for="exampleInputEmail1">{{{__ "subconstit"}}}</label>
                  <select class="form-control" id = "subcons">
                    <option value="all">الكل</option>
                  </select>
                </div>
              </form>
            </div>
            <div class="col-xs-2 {{{__ 'pullright'}}}">
              <form role="form">
                <div class="form-group">
                  <label for="mahallat">{{{__ "village"}}}</label>
                  <select class="form-control" id = "mahallat">
                    <option value="all">الكل</option>
                  </select>
                </div>
              </form>
            </div>
            <div class="col-xs-2 {{{__ 'pullright'}}}">
              <form role="form">
                <div class="form-group">
                  <label for="village">{{{__ "mahalla"}}}</label>
                  <select class="form-control" id = "village">
                    <option value="all">الكل</option>
                  </select>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
      <div class="row-fluid">
        <div dir = "{{{__ "dir"}}}">
          <div class="col-xs-2 {{{__ 'pullright'}}}">
            <form role="form">
              <div class="form-group">
                <label>{{{__ "search"}}}:</label>
                <input type="text" id="search" class="form-control" placeholder="{{{__ "searchNames"}}}">
              </div>
            </form>
          </div>
          <div class="col-xs-2 {{{__ 'pullright'}}}">
            <form role="form">
              <div class="form-group">
                <label>{{{__ "searchNames"}}}:</label>
                <input type="text" id="searchNames" class="form-control" placeholder="{{{__ "searchNames"}}}">
              </div>
            </form>
          </div>
          <div class="col-xs-2 {{{__ 'pullright'}}}">
            <form role="form">
              <div class="form-group">
                <label>{{{__ "searchMahalla"}}}:</label>
                <input type="text" id="searchMahalla" class="form-control" placeholder="{{{__ "searchMahalla"}}}">
              </div>
            </form>
          </div>
        </div>
      </div>
      <div class="row-fluid">
       <div class="col-md-8">
          <div dir="{{{__ "dir"}}}" id="map-canvas"><br><br><br><img class="img-responsive center-block" src="./img/rotate.gif"/></div>
        </div>
        <div class="table-responsive col-md-4" dir="{{{__ "dir"}}}">
            <table id="centerstable" class="table table-striped table-bordered table-condensed table-hover">
              <thead>
                <tr>
                  <th>{{{__ "centernumber"}}}</th>
                  <th>{{{__ "centername"}}}</th>
                  <th>{{{__ "mahalla"}}}</th>
                  <th>{{{__ "village"}}}</th>
                </tr>
              </thead>
              <tbody id = "centers">
                
              </tbody>
            </table>
            <ul class="pager">
              <li id ="prev"><a id="prevlink" href="#">{{{__ "previous"}}}</a></li>
              <li id ="next"><a id="nextlink" href="#">{{{__ "next"}}}</a></li>
            </ul>
        </div>
        
      </div>
      <div class="row-fluid">
        
      </div>
    </div>
  </div>
</div> <!-- /container -->


<script src="./js/jquery-1.10.2.min.js"></script>
<script src="./js/bootstrap.min.js"></script>
<script src="./js/index.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false&language={{{__ "lang"}}}"></script>

