<div class="container">
  <!-- Static navbar -->
  <div class="navbar navbar-default" role="navigation">
    <div class="container-fluid">
      <div class="navbar-header {{{__ "logo"}}}">
        <a class="navbar-brand" href="../../60/"><img alt="إحصائيات التسجيل" src="../../img/hnec_logo.png"></a>
      </div>
      <div class="navbar-collapse" dir="rtl" >
        <ul class="nav navbar-nav {{{__ "navbar"}}}">
          <li class="active"><a href="../../60/">{{{__ "main"}}}</a></li>
          <li><a href="http://hnec.ly/?page_id=2602" target="_blank">{{{__ "winners"}}}</a></li>
          <li><a href="http://hnec.ly">{{{__ "website"}}}</a></li>
        </ul>
      </div><!--/.nav-collapse -->
    </div><!--/.container-fluid -->
  </div>
  <div class="page-header">
    <h1>{{{__ "pageheader1"}}}<br> <small>{{{__ "pageheader2"}}}</small></h1>
  </div>

  <!-- Main component for a primary marketing message or call to action -->
  <legend class="{{{__ "textpos"}}}" dir="{{{__ "dir"}}}" >{{{__ "votingarea"}}}: {{transr vote_area}}</legend> 
  <div class="panel panel-default">
    <div class="panel-heading">
        <ol class="breadcrumb {{{__ "textpos"}}}" dir="{{{__ "dir"}}}">
        <li><a href="/60/">{{{__ "main"}}}</a></li>
        <li class="active">{{translate name_en name}}</li>
      </ol>
    </div>
    <div class="panel-body">
      <div class="table-responsive" dir="{{{__ "dir"}}}">
          <table class="table table-striped table-bordered table-condensed">
            <thead>
              <tr>
                <th>{{{__ "ballot"}}}</th>
                <th>{{{__ "validv"}}}</th>
                <th>{{{__ "seats"}}}</th>
                <th>{{{__ "candidatecount"}}}</th>
                <th>{{{__ "subconstit"}}}</th>
                <th>{{{__ "seconsubconstit"}}}</th>
                <th>{{{__ "station"}}}</th>
              </tr>
            </thead>
            <tbody>
              {{#each ballots}}
              <tr>
                <td>
                  <a href="/60/ballot/{{constit_id}}/{{ballot_id}}">
                    {{ballot_id}}-{{translate race_type race_type_ar}}
                  </a>
                </td>
                <td>{{valid_votes}}</td>
                <td>{{seats}}</td>
                <td>{{candidate_count}}</td>
                <td>{{transs subconst_name}}</td>
                <td>{{transs subsubconst_name}}</td>
                <td><a href="/60/centers/{{constit_id}}/{{ballot_id}}">{{{__ "stationsresults"}}}</a></td>
              </tr>
              {{/each}}
            </tbody>
          </table>
      </div>
    </div>
  </div>
</div> <!-- /container -->

<script src="{{{__ "js3"}}}"></script>
