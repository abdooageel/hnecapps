<div class="container">
  <!-- Static navbar -->
  <div class="navbar navbar-default" role="navigation">
    <div class="container-fluid">
      <div class="navbar-header {{{__ "logo"}}}">
        <a class="navbar-brand" href="../../"><img alt="إحصائيات التسجيل" src="../../../img/hnec_logo.png"></a>
      </div>
      <div class="navbar-collapse" dir="rtl" >
        <ul class="nav navbar-nav {{{__ "navbar"}}}">
          <li class="active"><a href="../../cor/">{{{__ "main"}}}</a></li>
          <li><a href="http://hnec.ly">{{{__ "website"}}}</a></li>
          <li><a href="../../../irr.pdf" target="_blank">{{{__ "irr"}}}</a></li>
        </ul>
      </div><!--/.nav-collapse -->
    </div><!--/.container-fluid -->
  </div>
  <div class="page-header">
    <h1>{{{__ "pageheader6"}}}<br> <small>{{{__ "pageheader8"}}}</small></h1>
  </div>

  <!-- Main component for a primary marketing message or call to action -->
    
  <div class="panel panel-default">
    <div class="panel-heading">
      <ol class="breadcrumb {{{__ "textpos"}}}" dir="{{{__ "dir"}}}">
        <li><a href="/cor/">{{{__ "main"}}}</a></li>
        <li><a href="/cor/constituency/{{constit_id}}">{{translate name_en name}}</a></li>
        <li><a href="/cor/centers/{{constit_id}}/{{ballot_id}}" >{{ballot.ballot_id}}{{translatetype ballot.race_type_ar}}</a></li>
        <li class="active">{{center_name}}</li>
      </ol>

    </div>
    <div class="panel-body">
      <div class="row-fluid">
        <div dir = "{{{__ "dir"}}}">
          <legend class="{{{__ "textpos"}}}" dir="{{{__ "dir"}}}" >{{{__ "centernumber"}}}: {{center_number}} - {{{__ "centername"}}}: {{center_name}}</legend>
        </div>
      </div>
      <!-- Nav tabs -->
      <ul class="nav nav-tabs">
        <li class="active"><a href="#center" data-toggle="tab">{{{__ "center"}}}</a></li>
        <li><a href="#stations" data-toggle="tab">{{{__ "station"}}}</a></li>
      </ul>
      <!-- Tab panes -->
      <div class="tab-content">
        <div class="tab-pane fade in active" id="center">
          <br>
          <div class="well">
          <table  class="table table-striped table-condensed table-bordered " dir="{{{__ "dir"}}}" >
            <thead>
              <tr>
                <th>#</th>
                <th>{{{__ "fullname"}}}</th>
                <th>{{{__ "validv"}}}</th>
              </tr>
            </thead>
            <tbody>
              {{#each center.candidates}}
              <tr >
                <td>{{number}}</td>
                <td>{{name}}</td>
                <td>{{votes}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
        </div>
        <div class="tab-pane fade" id="stations">
                <!-- Nav tabs -->
          <ul class="nav nav-tabs">
            {{#each center.stations}}
            <li class="{{active @index}}"><a href="#s{{station}}" data-toggle="tab">{{{__ "onestation"}}} : {{station}}</a></li>
            {{/each}}
          </ul>
          <!-- Tab panes -->
          <div class="tab-content">
            {{#each center.stations}}
            <div class="tab-pane fade {{active @index}}" id="s{{station}}">
              <table  class="table table-striped table-condensed table-bordered " dir="{{{__ "dir"}}}" >
                <thead>
                  <tr>
                    <th>#</th>
                    <th>{{{__ "fullname"}}}</th>
                    <th>{{{__ "validv"}}}</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each candidates}}
                  <tr >
                    <td>{{number}}</td>
                    <td>{{name}}</td>
                    <td>{{cvotes}}</td>
                  </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
            {{/each}}
            
          </div>
        </div>
      </div>
      
      
    </div>
  </div>
</div> <!-- /container -->

<script src="{{{__ "js3"}}}"></script>
