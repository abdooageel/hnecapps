        <li><a href="#stations" data-toggle="tab">{{{__ "station"}}}</a></li>
      </ul>
      <!-- Tab panes -->
      <div class="tab-content">
        <div class="tab-pane fade in active" id="center">
          <br>
          <div class="well">
          <table  class="table table-striped table-condensed table-bordered " dir="{{{__ "dir"}}}" >
            <thead>
              <tr>
                <th>#</th>
                <th>{{{__ "fullname"}}}</th>
                <th>{{{__ "validv"}}}</th>
              </tr>
            </thead>
            <tbody>
              {{#each center.candidates}}
              <tr >
                <td>{{number}}</td>
                <td>{{name}}</td>
                <td>{{votes}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
