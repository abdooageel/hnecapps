<script type="text/javascript">
  window.onload = function (){
    var myPDF = new PDFObject({
      url: "../../ResumeMar16.pdf",
      id: "myPDF",
      width: "93%",
      height: "400px",
      pdfOpenParams: {
        navpanes: 1,
        statusbar: 0,
        view: "FitH",
        pagemode: "thumbs"
      }
    }).embed("pdf");
  };
</script>